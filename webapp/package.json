  "version": "0.0.1",
  "scripts": {
    "start:node": "webpack --mode development --hot --watch --display minimal --config tools/webpack.config.node.js",
    "start:web": "webpack-dev-server --mode development --hot --watch --open --config tools/webpack.config.web.js",
    "start": "concurrently -k npm:start:node npm:start:web",
    "build": "webpack --mode production --progress --config tools/webpack.config.production.js",
    "migrate": "tsc -p src/migrations && node-pg-migrate --ignore-pattern '\\..*|.*\\.md'",
