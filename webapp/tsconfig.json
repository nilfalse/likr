{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { "~/*": ["src/*"] },
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": [ "es2015", "dom" ],
    "jsx": "react",
    "allowJs": true
  },
  "exclude": [
    "src/migrations/**"
  ]
}
